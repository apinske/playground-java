{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.0.1",
    "generators": {
      "playground-api": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/build/playground-api",
        "glob": "../resources/static/api.yaml",
        "additionalProperties": {
          "apiModulePrefix": "Playground"
        }
      },
      "camunda-api": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/build/camunda-api",
        "glob": "build/camunda-openapi.json",
        "additionalProperties": {
          "apiModulePrefix": "Camunda"
        }
      }
    }
  }
}
